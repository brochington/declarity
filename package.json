{
  "name": "declarity",
  "version": "0.2.5",
  "description": "A declarative way to organize application code",
  "main": "./build/bundle.js",
  "scripts": {
    "clean": "rimraf build",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\"",
    "tidy": "npm run lint:fix && npm run prettier",
    "build": "npm run clean && webpack",
    "watch": "npm run clean && webpack --watch",
    "link": "npm unlink && npm link",
    "rmrf": "rm -rf ./node_modules",
    "setup:build": "npm run link && npm run build",
    "setup:watch": "npm run link && npm run watch",
    "test": " ./node_modules/karma/bin/karma start",
    "test:travis": "./node_modules/karma/bin/karma start --single-run",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brochington/declarity.git"
  },
  "author": "Broch Stilley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brochington/declarity/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "lodash": "^4.13.1",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.2.1",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.52.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jsdoc": "^3.4.0",
    "jsdoc-babel": "^0.3.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^1.5.3",
    "rimraf": "^2.4.3",
    "sinon": "^3.2.0",
    "webpack": "^3.5.4"
  },
  "homepage": "https://github.com/brochington/declarity#readme"
}
