  "bugs": {
    "url": "https://github.com/brochington/declarity/issues"
  },
  "dependencies": {
    "immutable": "^3.7.5",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.0",
